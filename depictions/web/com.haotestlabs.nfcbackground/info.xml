<package>
	<id>com.haotestlabs.nfcbackground</id>
	<name>NFCBackground</name>
	<version>1.0.3</version>
	<compatibility>
		<firmware>
			<miniOS>12.0</miniOS>
			<maxiOS>12.4.1</maxiOS>
			<otherVersions>unsupported</otherVersions>
			<!--
			for otherVersions, you can put either unsupported or unconfirmed
			-->
		</firmware>
	</compatibility>
	<dependencies></dependencies>
	<descriptionlist>
		<description>Enables background reading of NFC tags on Iphone7 and up. This is mainly a helper tweak for others.</description>
	</descriptionlist>
	<screenshots></screenshots>
	<changelog>
		<change>Fixed a major issue, where it stopped working after one scan.</change>
	</changelog>
	<links></links>
</package>
